proc GetAttentionScriptParameters()
	var stmp$;
	var key$;
	key$:="AlertRig\\" + GetCurrentConfiguration$() + "\\Scripts\\AttentionGNG";
     
    attNumberOfBlocks% := GetIntRegistryValue%(key$, "NumberOfBlocks", 1);
