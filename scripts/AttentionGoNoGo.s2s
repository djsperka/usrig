    ColorVectorList$[1]:="Blue/Yellow";	cvl$[1]:="[0/0/1]-[1/1/0]";
    ColorVectorList$[2]:="Red/Cyan";	cvl$[2]:="[1/0/0]-[0/1/1]";
    ColorVectorList$[3]:="Green/Magenta";	cvl$[3]:="[0/1/0]-[1/0/1]";
